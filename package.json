{
	"name": "cwebp-bin",
	"version": "8.0.0",
	"description": "cwebp wrapper that makes it seamlessly available as a local dependency",
	"license": "MIT",
	"repository": "imagemin/cwebp-bin",
	"type": "module",
	"exports": "./index.js",
	"funding": "https://github.com/imagemin/cwebp-bin?sponsor=1",
	"bin": {
		"cwebp": "cli.js"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
		"test": "xo && ava --timeout=180s"
	},
	"files": [
		"cli.js",
		"index.js",
		"lib",
		"vendor/source"
	],
	"keywords": [
		"imagemin",
		"compress",
		"image",
		"img",
		"jpeg",
		"jpg",
		"minify",
		"optimize",
		"png",
		"webp"
	],
	"dependencies": {
		"@localnerve/bin-build": "^7.7.0",
		"@xhmikosr/bin-wrapper": "^13.0.5"
	},
	"devDependencies": {
		"ava": "^6.4.0",
		"@lesjoursfr/bin-check": "^7.1.9",
		"compare-size": "^3.0.0",
		"execa": "^9.6.0",
		"tempy": "^3.0.0",
		"xo": "^1.1.0"
	}
}
